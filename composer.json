{
    "name": "nazz/webpush",
    "description": "This is webPush library for fast run your messaging",
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.1.38 <6.0",
        "php-coveralls/php-coveralls": "dev-master"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Nazar Kaznadzey",
            "email": "nazar.kaznadzey@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Nazz\\WebPush\\": "src/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit -c ./tests/phpunit.xml ./tests",
        "cover": "./vendor/bin/phpunit -c ./tests/phpunit.xml --coverage-clover .build/logs/clover.xml ./tests",
        "coveralls": "./vendor/bin/coveralls -v"
    }
}
